<Window x:Class="ParametersDemo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ParametersDemo"
        xmlns:paramCtrls="clr-namespace:ParameterViews.UserControls;assembly=ParameterViews"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical">
        <StackPanel Orientation="Horizontal">
            <paramCtrls:ParamCollectionCtrl DataContext="{Binding}"/>
        </StackPanel>

        <StackPanel Orientation="Vertical">
                <Button Content="Bool prompts, no variables" Command="{Binding Prompt1Command}"/>
                <Button Content="Button2" Command="{Binding Prompt2Command}"/>
                <Button Content="Button3" Command="{Binding Prompt3Command}"/>
                <!-- Button Content="Button4" Command="{Binding Prompt4Command}"/>
                <Button Content="Button5" Command="{Binding Prompt5Command}"/ -->
        </StackPanel>

        <StackPanel Orientation="Horizontal">
            
        </StackPanel>
        </StackPanel>

    </Grid>
</Window>
